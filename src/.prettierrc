{
  "semi": true,
  "trailingComma": "es5",
  "printWidth": 120,
  "bracketSpacing": true,
  "overrides": [
    {
      "files": "*.html",
      "options": {
        "printWidth": 120,
        "parser": "html"
      }
    },
    {
      "files": "*.ts",
      "options": {
        "singleQuote": true
      }
    }
  ]
}
